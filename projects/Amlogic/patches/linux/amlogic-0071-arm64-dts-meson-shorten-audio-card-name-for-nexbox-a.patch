@@ -105,7 +105,7 @@
 
 	sound {
 		compatible = "amlogic,gx-sound-card";
-		model = "GXM-NEXBOX-A1";
+		model = "NEXBOX-A1";
 		assigned-clocks = <&clkc CLKID_MPLL0>,
 				  <&clkc CLKID_MPLL1>,
 				  <&clkc CLKID_MPLL2>;
